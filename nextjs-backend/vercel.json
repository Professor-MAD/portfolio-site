{
    "version": 2,
    "builds": [
      {
        "src": "pages/api/submit-message.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1",
        "headers": {
          "Access-Control-Allow-Origin": "https://portfolio-site-c4rd.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization"
        }
      }
    ]
  }
  